<?xml version='1.0' encoding='utf-8'?>
<widget id="ngs.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TsukuRepo</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <icon src="res/tsukurepo-icon.png" />
    <content src="index.html" />
    <access origin="*" />
    <access origin="content:///*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="res/ios/AppIcon.appiconset/Icon-App-60x60@3x.png" width="180" />
        <icon height="60" src="res/ios/AppIcon.appiconset/Icon-App-60x60@1x.png" width="60" />
        <icon height="120" src="res/ios/AppIcon.appiconset/Icon-App-60x60@2x.png" width="120" />
        <icon height="76" src="res/ios/AppIcon.appiconset/Icon-App-76x76@1x.png" width="76" />
        <icon height="152" src="res/ios/AppIcon.appiconset/Icon-App-76x76@2x.png" width="152" />
        <icon height="40" src="res/ios/AppIcon.appiconset/Icon-App-40x40@1x.png" width="40" />
        <icon height="80" src="res/ios/AppIcon.appiconset/Icon-App-40x40@2x.png" width="80" />
        <icon height="57" src="res/ios/AppIcon.appiconset/Icon-App-57x57@1x.png" width="57" />
        <icon height="114" src="res/ios/AppIcon.appiconset/Icon-App-57x57@2x.png" width="114" />
        <icon height="72" src="res/ios/AppIcon.appiconset/Icon-App-72x72@1x.png" width="72" />
        <icon height="144" src="res/ios/AppIcon.appiconset/Icon-App-72x72@2x.png" width="144" />
        <icon height="29" src="res/ios/AppIcon.appiconset/Icon-App-29x29@1x.png" width="29" />
        <icon height="58" src="res/ios/AppIcon.appiconset/Icon-App-29x29@2x.png" width="58" />
        <icon height="50" src="res/ios/AppIcon.appiconset/Icon-Small-50x50@1x.png" width="50" />
        <icon height="100" src="res/ios/AppIcon.appiconset/Icon-Small-50x50@2x.png" width="100" />
    </platform>
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <preference name="KeyboardShrinksView" value="true" />
    <engine name="android" spec="~5.0.0" />
    <engine name="ios" spec="~4.4.0" />
    <plugin name="cordova-plugin-camera" spec="~2.4.0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-geolocation" spec="~2.4.2" />
    <plugin name="cordova-plugin-keyboard" spec="~1.1.5" />
    <plugin name="cordova-plugin-customurlscheme" spec="~4.3.0">
        <variable name="URL_SCHEME" value="tsukurepo" />
    </plugin>
    <plugin name="cordova-plugin-device" spec="~1.1.6-dev" />
    <plugin name="cordova-plugin-fcm" spec="~2.1.2" />
    <plugin name="cordova-banner-notification" spec="~0.1.6" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.3" />
</widget>
